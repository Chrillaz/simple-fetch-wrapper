{
  "name": "simple-fetch-wrapper",
  "version": "1.3.7",
  "description": "Fetch wrapper",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc --p ./tsconfig.json --watch",
