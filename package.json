    "fetch",
    "http",
    "ajax"
  ],
  "author": "Christoffer Ã–hman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chrillaz/simple-fetch-wrapper/issues"
  },
  "homepage": "https://github.com/Chrillaz/simple-fetch-wrapper#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
